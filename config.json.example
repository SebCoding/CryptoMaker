     "limit_entry": {
         "abort_price_pct": 0.005,
         "abort_time_candle_ratio": 10.0,
         "fixed_tp_throughout_trade": true
     },
     "database": {
         "db_name": "CryptoMakerTest",
         "address": "localhost",
         "port": 5432,
         "username": "CryptoMakerUser",
         "password": "db_password"
     },
    "logging": {
        "logging_level": "info",
        "debug_file_path": "logging_/debug_log.txt",
        "output_file_path": "logging_/output_log.txt"
    }
